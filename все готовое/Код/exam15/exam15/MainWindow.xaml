<Window x:Class="exam15.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:exam15"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <Image Source="/Resources/Образ плюс.ico" HorizontalAlignment="Left"></Image>
        <TextBlock Text="Образ плюс" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="30" FontFamily="Constantia"></TextBlock>
        <Button Content="Вернуться" HorizontalAlignment="Right" VerticalAlignment="Center" Name="BtnBack" Click="BtnBack_Click"></Button>
        <Frame Grid.Row="1" NavigationUIVisibility="Hidden" Name="MainFrame" ContentRendered="MainFrame_ContentRendered"></Frame>
    </Grid>
</Window>
